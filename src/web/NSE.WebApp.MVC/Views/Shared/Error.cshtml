@model NSE.WebApp.MVC.Models.Error.ErrorViewModel
@{
    ViewData["Title"] = @Model.Title;
}

<div class="container">
    <div style="padding-top: 200px; text-align: center">
        @{
            if (Model == null)
            {
                <h2>Ooops! Ocorreu um erro, mas não se preocupe. Nosso time será avisado e iremos conrrigir em breve!</h2>
            }
            else
            {
                <h2>@Html.Raw(Model.Message)</h2>
            }
        }
    </div>
</div>

